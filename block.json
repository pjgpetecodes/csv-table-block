{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "petecodes/csv-table-block",
    "title": "CSV Table Block",
    "description": "A Gutenberg block for importing and displaying CSV data as a table.",
    "version": "1.0.0",
    "category": "widgets",
    "keywords": [
      "csv",
      "table",
      "block",
      "gutenberg"
    ],
    "supports": {
      "html": false,
      "align": true
    },
    "attributes": {
      "tableHTML": {
        "type": "string",
        "source": "html",
        "selector": "div.csv-table-block"
      }
    },
    "style": "csv-table-block-front-style",
    "editorScript": "file:./block.js"
  }
  